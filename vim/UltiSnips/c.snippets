snippet p "printf"
printf("$1${2: %i}\n", $3);
endsnippet

snippet if "if"
if ($1) {
	$2;
}
endsnippet

snippet for "for"
for (${1:int }$2 = 0; $2 < $3; $2++) {
	$4;
}
endsnippet

snippet in "#include"
#include <$1>
endsnippet

snippet ts "typedef struct"
typedef struct $1 {
	$2;
} $1;
endsnippet

snippet s "switch"
switch ($1) {
case $2:
	$3;
default:
	$4;
endsnippet

snippet f "function"
${1:void} $2(${3:void}) {
	$4;
} 
endsnippet


